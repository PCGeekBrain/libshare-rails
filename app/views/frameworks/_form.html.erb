<%= form_for(@framework) do |f| %>
    <%= f.label :name, "Framework Name: "%>
    <%= f.text_field :name, class: "form-control", placeholder: "Framework name" %><br/>
    <%= f.label :name, "Framework Description: "%>
    <%= f.text_area :description, class: "form-control", placeholder: "Framework Description" %><br/>
    <%= f.submit class: "btn btn-primary" %>
<% end %>